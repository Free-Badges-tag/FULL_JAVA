public class Bitwise{
    public static void main(String args[]){
        int p=10;
        int q=8;
        System.out.println(p|q);
        System.out.println(p&q);
        System.out.println(p^q);

        System.out.println(p<<1);
        System.out.println(p<<2);
        System.out.println(q>>1);
        System.out.println(q>>2);
        System.out.println(q>>3);

    }
}